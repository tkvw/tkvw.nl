{
  "version": 1,
  "projects": {
    "www": {
      "root": "apps/www",
      "sourceRoot": "apps/www/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/www/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "www:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "www:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/www/tsconfig.json",
              "apps/www/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/www/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/www/jest.config.js",
            "tsConfig": "apps/www/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "www-e2e": {
      "root": "apps/www-e2e",
      "sourceRoot": "apps/www-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/www-e2e/cypress.json",
            "tsConfig": "apps/www-e2e/tsconfig.e2e.json",
            "devServerTarget": "www:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "www:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/www-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/www-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "schematics": {
    "@nrwl/gatsby": {
      "application": {}
    }
  },
  "defaultProject": "www"
}
